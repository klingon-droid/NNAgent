# SYMBaiEX API Configuration
VITE_SYMBAIEX_API_KEY=your-api-key-here
# Optional: Override auto-detected API endpoints
# VITE_SYMBAIEX_API_URL=http://localhost:3000/api/v1
# VITE_SYMBAIEX_WS_URL=ws://localhost:3000/api/v1/ws

# AI Provider Configuration
VITE_AI_PROVIDER=galadriel  # galadriel, openai, anthropic, heuristic, ollama
VITE_NORMAL_MODEL=hermes3:3b  # Default model for normal interactions
VITE_LARGE_MODEL=hermes3:7b  # Model for complex tasks

# Provider API Keys
VITE_OPENAI_API_KEY=your-openai-key
VITE_ANTHROPIC_API_KEY=your-anthropic-key
VITE_HEURISTIC_API_KEY=your-heuristic-key
VITE_GALADRIEL_API_KEY=your-galadriel-key

# Ollama Configuration (for local deployment)
VITE_OLLAMA_HOST=http://localhost:11434

# Terminal Configuration
VITE_TERMINAL_HISTORY_SIZE=100
VITE_TERMINAL_MAX_OUTPUT=1000

# Rate Limiting
VITE_RATE_LIMIT_MAX_REQUESTS=20
VITE_RATE_LIMIT_WINDOW_MS=900000  # 15 minutes in milliseconds